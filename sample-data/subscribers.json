{
  "subscribers": [
    {
      "id": "sub-credentialing-system",
      "name": "Credentialing Management System",
      "description": "Enterprise credentialing and privileging system that monitors provider credentials and licenses",
      "endpoint": {
        "resourceType": "Endpoint",
        "id": "endpoint-001",
        "status": "active",
        "connectionType": {
          "system": "http://terminology.hl7.org/CodeSystem/endpoint-connection-type",
          "code": "rest-hook",
          "display": "REST Hook"
        },
        "name": "Credentialing System Webhook",
        "address": "https://credentialing-system.example.com/webhooks/provider-updates",
        "payloadType": [
          {
            "coding": [
              {
                "system": "http://hl7.org/fhir/resource-types",
                "code": "Practitioner"
              }
            ]
          },
          {
            "coding": [
              {
                "system": "http://hl7.org/fhir/resource-types",
                "code": "PractitionerRole"
              }
            ]
          }
        ],
        "payloadMimeType": ["application/fhir+json"],
        "webhookConfig": {
          "eventTypes": [
            "practitioner.created",
            "practitioner.updated",
            "practitionerRole.updated",
            "license.expiring",
            "license.expired",
            "credentialing.statusChange"
          ],
          "retryPolicy": {
            "maxAttempts": 5,
            "backoffStrategy": "exponential",
            "initialDelayMs": 1000,
            "maxDelayMs": 60000,
            "retryableStatusCodes": [408, 429, 500, 502, 503, 504]
          },
          "timeout": 30000,
          "includeFullResource": true,
          "signatureAlgorithm": "HMAC-SHA256",
          "secret": "cred-system-secret-key-12345",
          "batchingEnabled": false
        },
        "authentication": {
          "type": "api-key",
          "credentials": {
            "apiKey": "cred-api-key-abcdef123456"
          },
          "headers": [
            {
              "key": "X-API-Key",
              "value": "cred-api-key-abcdef123456"
            }
          ]
        },
        "rateLimit": {
          "requestsPerMinute": 100,
          "requestsPerHour": 5000,
          "requestsPerDay": 50000,
          "burstLimit": 200
        },
        "monitoring": {
          "enabled": true,
          "healthCheckUrl": "https://credentialing-system.example.com/health",
          "healthCheckIntervalMs": 300000,
          "alertOnFailure": true,
          "alertEmail": ["ops@credentialing-system.example.com", "admin@credentialing-system.example.com"],
          "successRate": {
            "threshold": 98,
            "windowMinutes": 60
          }
        }
      },
      "subscriptionDetails": {
        "purpose": "Monitor provider credentials, licenses, and board certifications",
        "dataUsage": "Credentialing verification and compliance monitoring",
        "contactEmail": "integrations@credentialing-system.example.com",
        "technicalContact": "api-support@credentialing-system.example.com",
        "businessContact": "credentialing-team@credentialing-system.example.com"
      }
    },
    {
      "id": "sub-directory-service",

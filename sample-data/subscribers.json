        "name": "Directory Service Webhook",
        "address": "https://provider-directory.example.com/api/v1/webhooks/updates",
        "payloadType": [
          {
            "coding": [
              {
                "system": "http://hl7.org/fhir/resource-types",
                "code": "Practitioner"
              }
            ]
          },
          {
            "coding": [
              {

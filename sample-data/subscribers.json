          },
          "timeout": 15000,
          "includeFullResource": true,
          "signatureAlgorithm": "HMAC-SHA512",
          "secret": "directory-secret-key-xyz789",
          "batchingEnabled": true,
          "batchSize": 50,
          "batchWindowMs": 5000
        },
        "authentication": {
          "type": "bearer",
          "credentials": {
            "token": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "headers": [
            {
              "key": "Authorization",
              "value": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
            }
          ]
        },
        "rateLimit": {
          "requestsPerMinute": 200,
          "requestsPerHour": 10000,
          "requestsPerDay": 100000,
          "burstLimit": 500
        },
        "monitoring": {
          "enabled": true,
          "healthCheckUrl": "https://provider-directory.example.com/health/webhook",
          "healthCheckIntervalMs": 180000,
          "alertOnFailure": true,
          "alertEmail": ["devops@provider-directory.example.com"],
          "successRate": {

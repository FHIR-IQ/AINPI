# Vercel Postgres connection strings
# These will be auto-populated by Vercel when you connect a database
# For local dev, you can use a local Postgres instance or Vercel dev database

# Connection pooling URL (recommended for serverless)
POSTGRES_PRISMA_URL="postgresql://placeholder:placeholder@localhost:5432/providercard?pgbouncer=true"

# Direct connection URL (for migrations)
POSTGRES_URL_NON_POOLING="postgresql://placeholder:placeholder@localhost:5432/providercard"

# Legacy support (will use POSTGRES_PRISMA_URL if not set)
DATABASE_URL="postgresql://placeholder:placeholder@localhost:5432/providercard"
